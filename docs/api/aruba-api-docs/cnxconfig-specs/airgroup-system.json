    "schemas": {
      "ArubaAirgroupSystem_AirgroupSystemSchema": {
        "x-supportedDeviceType": [
          "Access Point",
          "Gateway"
        ],
        "description": "AirGroup is a unique service that uses zero-configuration networking\n and allows devices to communicate over complex access-network topologies.\n AirGroup supports Bonjour and DLNA services on supported devices.\n This feature is applicable for AP",
        "allOf": [
          {
            "$ref": "#/components/schemas/ArubaAirgroupSystem_IapAirgroup"
          }
        ],
        "properties": {
          "enable": {
            "x-supportedDeviceType": [
              "Access Point",
              "Gateway"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enable Airgroup feature"
          },
          "protocol": {
            "x-supportedDeviceType": [
              "Access Point"
            ],
            "type": "string",
            "enum": [
              "ALL",
              "MDNS_ONLY",
              "DLNA_ONLY"
            ],
            "format": "enum",
            "x-enumDescriptions": {
              "ALL": "Airgroup supported for both MDNS and DLNA service",
              "MDNS_ONLY": "Airgroup supported only for MDNS service",
              "DLNA_ONLY": "Airgroup supported only for DLNA service"
            },
            "x-typeDescription": "Airgroup protocol level enabling options",
            "default": "ALL",
            "description": "This option is used to enable only specific protocol for Airgroup feature, by default both MDNS and SSDP is supported"
          }
        }
      },
      "ArubaAirgroupSystem_IapAirgroup": {
        "description": "Grouping for Instant AP AirGroup Configuration",
        "properties": {
          "cppm": {
            "description": "Airgroup ClearPass Policy Manager Configuration Container",
            "x-supportedDeviceType": [
              "Access Point"
            ],
            "properties": {
              "enforce-registration": {
                "x-supportedDeviceType": [
                  "Access Point"
                ],
                "type": "boolean",
                "default": "false",
                "description": "When enabled, only devices registered with ClearPass Policy Manager will be discovered by Bonjour devices"
              },
              "query-interval": {
                "x-supportedDeviceType": [
                  "Access Point"
                ],
                "type": "integer",
                "format": "int64",
                "minimum": 1,
                "maximum": 24,
                "description": "ClearPass Policy Manager query interval information for AirGroup policy."
              },
              "server": {
                "description": "Airgroup ClearPass Policy Manager server configuration",
                "type": "array",
                "items": {
                  "properties": {
                    "name": {
                      "x-supportedDeviceType": [
                        "Access Point"
                      ],
                      "x-path": "/ac-auth-server:auth-servers/ac-auth-server:auth-server/ac-auth-server:name",
                      "type": "string",
                      "format": "const",
                      "minLength": 1,
                      "maxLength": 63,
                      "description": "ClearPass Policy Manager server information for AirGroup policy."
                    }
                  }
                },
                "x-key": [
                  "name"
                ],
                "maxItems": 2147483647
              }
            }
          },
          "guest-multicast": {
            "x-supportedDeviceType": [
              "Access Point"
            ],
            "type": "boolean",
            "default": "false",
            "description": "When enabled, Bonjour devices are visible only in guest VLAN and AirGroup will not discover or enforce policies in guest VLAN."
          },
          "multi-swarm": {
            "x-supportedDeviceType": [
              "Access Point"
            ],
            "type": "boolean",
            "default": "false",
            "description": "When enabled, the Instant AP shares the mDNS/SSDP database information with the other clusters."
          }
        }
      },
