    "schemas": {
      "ArubaHotspot2H2qpConnCap_H2qpConnCapSchema": {
        "description": "This is used to configure a Hotspot 2.0 Query Protocol (H2QP) profile that advertises hotspot protocol and port capabilities.\nThe values configured in this profile can be sent in an Access network Query Protocol (ANQP) IE to provide hotspot clients\ninformation about the IP protocols and associated port numbers that are available and open for\ncommunication.",
        "properties": {
          "h2qp-conn-cap-profile": {
            "description": "List of H2QP profiles that advertises hotspot protocol and port capabilities.",
            "type": "array",
            "items": {
              "properties": {
                "name": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "string",
                  "format": "const",
                  "minLength": 1,
                  "maxLength": 64,
                  "description": "Profile name."
                },
                "enable": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "true",
                  "description": "Enable this profile."
                },
                "icmp": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Indicates that the ICMP port is enabled and available. (port 0)"
                },
                "tcp-ftp": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the FTP port. (port 20)"
                },
                "tcp-ssh": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the SSH port. (port 22)"
                },
                "tcp-http": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the HTTP port. (port 80)"
                },
                "tcp-tls-vpn": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the TCP TLS port used by VPNs. (port 80)"
                },
                "tcp-pptp-vpn": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the PPTP port used by IPsec VPNs. (port 1723)"
                },
                "tcp-voip": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the TCP VoIP port. (port 5060)"
                },
                "udp-voip": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Include this parameter to enable the UDP VoIP port. (port 5060)"
                },
                "udp-ipsec-vpn": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Enable UDP IKEv2 port 500 for IPSec VPN."
                },
                "udp-ike2": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Enable UDP IKEv2 port 4500 for IPSec VPN."
                },
                "esp-port": {
                  "x-supportedDeviceType": [
                    "Access Point"
                  ],
                  "type": "boolean",
                  "default": "false",
                  "description": "Enable ESP port Used by IPSec VPN."
                }
              }
            },
            "x-key": [
              "name"
            ],
            "maxItems": 2147483647
          }
        }
      },
