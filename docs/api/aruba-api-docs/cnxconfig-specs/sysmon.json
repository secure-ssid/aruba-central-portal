            "description": "System monitor configuration profile.",
            "type": "array",
            "items": {
              "properties": {
                "name": {
                  "type": "string",
                  "format": "const",
                  "minLength": 1,
                  "maxLength": 256,
                  "description": "System monitor profile name."
                },
                "description": {
                  "type": "string",
                  "format": "const",
                  "minLength": 1,
                  "maxLength": 256,
                  "description": "System monitor profile description."
