      "ArubaDhcpRelay_Option82": {
        "description": "Grouping for Option 82 Configurations",
        "properties": {
          "option-82": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "description": "Option 82 Configurations",
            "properties": {
              "enable": {
                "x-supportedDeviceType": [
                  "Switch CX"
                ],
                "type": "boolean",
                "default": "false",
                "description": "Enables the use of the configured source-interface for DHCPv4 relay agent.\nWhen set to true, the configured source-interface is used as source IP address while\nsending DHCPv4 relay messages to server."
              },
              "policy": {
                "x-supportedDeviceType": [
                  "Switch PVOS",
                  "Switch CX"
                ],
                "type": "string",
                "enum": [
                  "DROP",
                  "KEEP",
                  "REPLACE"
                ],
                "format": "enum",
                "x-enumDescriptions": {
                  "DROP": "Drop DHCP packets with option 82 information.",
                  "KEEP": "Retain option 82 information in the DHCP packets.",
                  "REPLACE": "Replace option 82 information in the DHCP packets."
                },
                "x-typeDescription": "Option-82 Policy types.",
                "default": "REPLACE",
                "description": "Policy to be applied on the client DHCP packets with option 82."
              },
              "validate": {
                "x-supportedDeviceType": [
                  "Switch CX",
                  "Switch PVOS"
                ],
                "type": "boolean",
                "default": "false",
                "description": "Validates option 82 information in the DHCP server response packets."
              },
              "remote-id": {
                "x-supportedDeviceType": [
                  "Switch PVOS",
                  "Switch CX"
                ],
                "type": "string",
                "enum": [
                  "IP",
                  "MAC",
                  "IP_MGMT_VLAN"
                ],
                "format": "enum",
                "x-enumDescriptions": {
                  "IP": "IP address of client interface",
                  "MAC": "MAC address of the switch.",
                  "IP_MGMT_VLAN": "IP address of the management VLAN"
                },
                "x-typeDescription": "Option-82 remote id",
                "default": "MAC",
                "description": "Option 82 remote ID sub-option to be sent in the DHCP packet to server.\nIt is used to identify the relay agent device."
              },
              "source-interface": {
                "x-supportedDeviceType": [
                  "Switch CX"
                ],
                "type": "boolean",
                "default": "false",
                "description": "Enables the use of the configured source-interface for DHCPv4 relay agent communication.\nWhen set to true, the configured source-interface will be used for sending DHCPv4 relay messages."
              }
