      "ArubaDhcpRelay_Ipv4Relay": {
        "description": "Grouping for DHCPv4 Relay Configurations",
        "allOf": [
          {
            "$ref": "#/components/schemas/ArubaDhcpRelay_Option82"
          }
        ],
        "properties": {
          "enable": {
            "x-supportedDeviceType": [
              "Switch PVOS",
              "Switch CX"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enables DHCPv4 relay globally. The functionality is effective\n  \t only after the port level helper configuration."
          },
          "hop-count-increment": {
            "x-supportedDeviceType": [
              "Switch PVOS",
              "Switch CX"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enables hop count increment in the relayed DHCPv4 requests."
          },
          "smart-relay": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enables DHCPv4 smart relay globally. When multiple IPv4 addresses are configured on an\ninterface, the primary IP address is used first as the gateway IP (GIADDR) for DHCP relay.\nIf the DHCP server does not have available IP addresses in the pool corresponding to the primary\nIP's subnet, the secondary IP address is used as a fallback. This ensures that the DHCP relay\nremains functional by switching to a secondary IP address when the primary IP's subnet pool\nis exhausted."
          },
          "l2vpn-clients": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enables DHCPv4 relay for remote L2 VPN clients."
          },
          "reply-overwrite-source-ip": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enables DHCPv4 relay overwrite source IP."
          },
          "option-xml": {
            "x-supportedDeviceType": [
              "Gateway"
            ],
            "type": "string",
            "format": "const",
            "minLength": 1,
            "maxLength": 65,
            "description": "IPv4 DHCP relay option XML file. The XML file is used as the input from the user and is validated\nagainst an XSD file in the controller. The format in the XML file is parsed and stored in the DHCP\nrelay which is used to insert Option-82 related values in the DHCP request packets sent from the\nclient to the server."
          }
        }
      },
