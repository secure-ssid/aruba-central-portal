      "ArubaClientIptracker_GlobalConfig": {
        "description": "Grouping for Global-level configuration parameters of client IP tracker.",
        "properties": {
          "enable": {
            "x-supportedDeviceType": [
              "Switch PVOS",
              "Switch CX"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enables client IP tracking on the Switch. This is applicable to AOS-CX and AOS-S Switches."
          },
          "enable-all-vlans": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enables client IP tracking on all the VLANs of the AOS-CX Switch."
          },
          "probe-delay": {
            "x-supportedDeviceType": [
              "Switch PVOS"
            ],
            "type": "integer",
            "format": "int32",
            "minimum": 15,
            "maximum": 300,
            "description": "Time delay in seconds after which clients on this port will be\nARP probed for activity in the data path on the AOS-S Switch."
          },
          "trust-mode": {
            "x-supportedDeviceType": [
              "Switch PVOS"
            ],
            "type": "string",
            "enum": [
              "TRUSTED",
              "UNTRUSTED"
            ],
            "format": "enum",
            "x-enumDescriptions": {
              "TRUSTED": "Enables or disables the visibility of statically and\ndynamically assigned IPv4 and IPv6 addresses for\nauthenticated clients on the AOS-S switch.",
              "UNTRUSTED": "Enables or disables the visibility of statically and\ndynamically assigned IPv4 and IPv6 addresses for\nnon-authenticated clients on the AOS-S switch."
            },
            "x-typeDescription": "Specifies the trust mode configuration of client IP tracking feature on the AOS-S Switch.",
            "description": "Specifies the Trust mode on the AOS-S Switch."
          },
          "enable-probe": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enable client IP probe(using ARP) to refresh the IP address of the clients. This is applicable to AOS-CX switch."
          }
        }
      },
