    "schemas": {
      "ArubaCdaAirpassApproval_AirpassApprovalSchema": {
        "description": "Air Pass enables seamless cellular roaming to enterprise networks.\nAir Pass approval defines site based approval requests for MNOs with the approval status.\n\nThis is a read-only configuration",
        "properties": {
          "approval": {
            "description": "Air Pass approvals for Central NAC.\n\nAir Pass approval defines site based approval requests for MNOs with the approval status.",
            "type": "array",
            "items": {
              "properties": {
                "name": {
                  "type": "string",
                  "format": "const",
                  "minLength": 1,
                  "maxLength": 255,
                  "description": "Name of Air Pass approval profile."
                },
                "site-id": {
                  "type": "string",
                  "format": "pattern",
                  "minLength": 1,
                  "maxLength": 255,
                  "x-patternSources": "scope-id-type",
                  "x-patterns": "'[0-9]*'",
                  "x-typeDescription": "it is a unique ID given to a scope, it is unique across a particular construct",
                  "x-mandatory": true,
                  "description": "Site id."
                },
                "mno": {
                  "type": "string",
                  "enum": [
                    "ATT",
                    "TMO",
                    "VRZ"
                  ],
                  "format": "enum",
                  "x-enumDescriptions": {
                    "ATT": "AT&T.",
                    "TMO": "T-Mobile.",
                    "VRZ": "Verizon."
                  },
                  "x-typeDescription": "Type of Mobile Network Operator.",
                  "x-mandatory": true,
                  "description": "Mobile Network Operator (MNO) name"
                },
                "airpass-profile-id": {
                  "type": "string",
                  "format": "pattern",
                  "x-patternSources": "airpass-profile-id",
                  "x-patterns": "'[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}'",
                  "maxLength": 9999,
                  "x-mandatory": true,
                  "description": "Unique immutable identifier for associated Air Pass auth profile."
                },
                "approval-status": {
                  "type": "string",
                  "enum": [
                    "PENDING",
                    "APPROVED",
                    "REJECTED"
                  ],
                  "format": "enum",
                  "x-enumDescriptions": {},
                  "x-typeDescription": "Approval status of the MNO request.",
                  "x-mandatory": true,
                  "description": "Approval status from MNO for the site."
                },
                "request-ts": {
                  "type": "string",
                  "format": "pattern",
                  "x-patternSources": "date-and-time",
                  "x-patterns": "'[0-9]{4}\\-(0[1-9]|1[0-2])\\-(0[1-9]|[12][0-9]|3[01])[Tt](0[0-9]|1[0-9]|2[0-3]):(0[0-9]|[1-5][0-9]):(0[0-9]|[1-5][0-9]|60)(\\.[0-9]+)?([Zz]|([+-](0[0-9]|1[0-9]|2[0-3]):(0[0-9]|[1-5][0-9])))'",
                  "maxLength": 9999,
                  "x-typeDescription": "A date and time, expressed in the format described in RFC3339.\nThat is to say:\n\nYYYY-MM-DDTHH:MM:SSZ+-hh:mm\n\nwhere YYYY is the year, MM is the month expressed as a two-digit\nmonth (zero padding if required), DD is the day of the month,\nexpressed as a two digit value. T is the literal character 'T',\nHH is the hour of the day expressed as a two digit number, using\nthe 24-hour clock, MM is the minute of the hour expressed as a\ntwo digit number. Z is the literal character 'Z', followed by a\ntimezone offset expressed in hours (hh) and minutes (mm), both\nexpressed as two digit numbers. The time offset is specified as\na positive or negative offset to UTC using the '+' or '-'\ncharacter preceding the offset.\n\nOptionally, fractional seconds can be expressed after the minute\nof the hour as a decimal number of unspecified precision\nreflecting fractions of a second.",
                  "description": "Timestamp for when the approval request was generated."
                },
                "response-ts": {
                  "type": "string",
                  "format": "pattern",
                  "x-patternSources": "date-and-time",
                  "x-patterns": "'[0-9]{4}\\-(0[1-9]|1[0-2])\\-(0[1-9]|[12][0-9]|3[01])[Tt](0[0-9]|1[0-9]|2[0-3]):(0[0-9]|[1-5][0-9]):(0[0-9]|[1-5][0-9]|60)(\\.[0-9]+)?([Zz]|([+-](0[0-9]|1[0-9]|2[0-3]):(0[0-9]|[1-5][0-9])))'",
                  "maxLength": 9999,
                  "x-typeDescription": "A date and time, expressed in the format described in RFC3339.\nThat is to say:\n\nYYYY-MM-DDTHH:MM:SSZ+-hh:mm\n\nwhere YYYY is the year, MM is the month expressed as a two-digit\nmonth (zero padding if required), DD is the day of the month,\nexpressed as a two digit value. T is the literal character 'T',\nHH is the hour of the day expressed as a two digit number, using\nthe 24-hour clock, MM is the minute of the hour expressed as a\ntwo digit number. Z is the literal character 'Z', followed by a\ntimezone offset expressed in hours (hh) and minutes (mm), both\nexpressed as two digit numbers. The time offset is specified as\na positive or negative offset to UTC using the '+' or '-'\ncharacter preceding the offset.\n\nOptionally, fractional seconds can be expressed after the minute\nof the hour as a decimal number of unspecified precision\nreflecting fractions of a second.",
                  "description": "Timestamp for when the approval response was received."
                }
              },
              "required": [
                "site-id",
                "mno",
                "airpass-profile-id",
                "approval-status"
              ]
            },
            "x-key": [
              "name"
            ],
            "maxItems": 2147483647
          }
        }
      },
