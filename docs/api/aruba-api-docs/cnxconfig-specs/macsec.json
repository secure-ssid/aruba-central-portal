      "ArubaMacsec_MacsecPolicyConfig": {
        "description": "Grouping for MACsec policy configurations.",
        "properties": {
          "confidentiality-enable": {
            "x-supportedDeviceType": [
              "Switch CX",
              "Switch PVOS"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enable confidentiality in the MACsec policy."
          },
          "confidentiality-offset": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "string",
            "enum": [
              "BYTE_0",
              "BYTE_30",
              "BYTE_50"
            ],
            "format": "enum",
            "x-enumDescriptions": {
              "BYTE_0": "The entire Ethernet frame is sent encrypted.",
              "BYTE_30": "The data following the first 30 bytes of the Ethernet\nframe is sent encrypted.",
              "BYTE_50": "The data following the first 50 bytes of the Ethernet\nframe is sent encrypted."
            },
            "x-typeDescription": "Confidentiality offset type.",
            "default": "BYTE_0",
            "description": "Number of octets in an Ethernet frame that are unencrypted.\nThis is only applicable when confidentiality is enabled in the\npolicy."
          },
          "replay-protect-enable": {
            "x-supportedDeviceType": [
              "Switch CX",
              "Switch PVOS"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enable replay protection in the MACsec policy."
          },
          "replay-window": {
            "x-supportedDeviceType": [
              "Switch CX",
              "Switch PVOS"
            ],
            "type": "integer",
            "format": "int64",
            "minimum": 0,
            "maximum": 4294967295,
            "default": "0",
            "description": "Replay protection window associated with this policy. When a\npacket is received, it is processed only if the Packet Number\n(PN) associated with this packet is within the replay window.\nThis is only applicable if the replay protection is enabled in\nthe policy.\nFor PVOS device, valid range: 0-1024.\nFor CX device, valid range: 0-4294967295."
          },
          "include-sci-enable": {
            "x-supportedDeviceType": [
              "Switch CX",
              "Switch PVOS"
            ],
            "type": "boolean",
            "default": "true",
            "description": "Enable inclusion of Secure Channel Identifier (SCI) tag\ninformation in the Security TAG (SecTAG) field."
          },
          "cipher-suites": {
            "type": "array",
            "items": {
              "x-supportedDeviceType": [
                "Switch CX"
              ],
              "type": "string",
              "enum": [
                "GCM_AES_128",
                "GCM_AES_256",
                "GCM_AES_128_XPN",
                "GCM_AES_256_XPN"
              ],
              "format": "enum",
              "x-enumDescriptions": {
                "GCM_AES_128": "Galois/Counter mode of operation with the AES-128 symmetric block\ncipher.",
                "GCM_AES_256": "Galois/Counter mode of operation with the AES-256 symmetric block\ncipher.",
                "GCM_AES_128_XPN": "Galois/Counter mode of operation with the AES-128 symmetric block\ncipher with extended packet numbering.",
                "GCM_AES_256_XPN": "Galois/Counter mode of operation with the AES-256 symmetric block\ncipher with extended packet numbering."
              },
              "x-typeDescription": "Cipher suite type",
              "description": "Configure the cipher-suites to use in the MACsec policy."
            },
            "maxItems": 2147483647
          },
          "secure-mode": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "string",
            "enum": [
              "SHOULD_SECURE",
              "MUST_SECURE"
            ],
            "format": "enum",
            "x-enumDescriptions": {
              "SHOULD_SECURE": "Configure the interface to allow traffic if MKA session is not\nestablished.",
              "MUST_SECURE": "Configure the interface to block traffic if MKA session is not\nestablished."
            },
            "x-typeDescription": "Secure mode type.",
            "default": "MUST_SECURE",
            "description": "Forwarding behavior of the interface when\nthe MKA session is not established."
          },
          "data-delay-protection": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "boolean",
            "default": "false",
            "description": "Enable data delay protection. Data delay protection allows\nMKA participants to ensure that the data frames protected by\nMACsec delayed by more than 2 seconds are dropped."
          },
          "bypass-list": {
            "type": "array",
            "items": {
              "x-supportedDeviceType": [
                "Switch CX"
              ],
              "type": "string",
              "enum": [
                "IEEE_BPDU"
              ],
              "format": "enum",
              "x-enumDescriptions": {
                "IEEE_BPDU": "Bypass MACsec protection for IEEE BPDU frames.\nPackets with destination MAC as one of the IEEE BPDU MACs\n(01:80:c2:00:00:0*) will bypass the MACsec engine in both the Tx and\nRx directions."
              },
              "x-typeDescription": "Bypass packet type.",
              "description": "List of packet types to bypass MACsec."
            },
            "maxItems": 2147483647
          },
          "clear-tag-mode": {
            "x-supportedDeviceType": [
              "Switch CX"
            ],
            "type": "string",
            "enum": [
              "NONE",
              "DOT1Q"
            ],
            "format": "enum",
            "x-enumDescriptions": {
              "NONE": "The SecTAG directly follows the destination and source\nMAC address in a MACsec PDU.",
              "DOT1Q": "Send the 802.1q tag in clear in a MACsec PDU.\nWhen configured, untagged traffic is not allowed on the\nMACsec channel."
            },
            "x-typeDescription": "Clear-tag mode type.",
            "default": "NONE",
            "description": "Ethernet data in a MACsec PDU that must precede the MACsec SecTAG\nin clear text."
          },
          "ckn": {
            "x-supportedDeviceType": [
              "Switch PVOS"
            ],
            "type": "string",
            "format": "pattern",
            "x-patternSources": "ckn",
            "x-patterns": "'[a-f0-9]{1,64}'",
            "maxLength": 9999,
            "description": "Connectivity Association Key Name (CKN) associated with this\npolicy."
          },
          "cak": {
            "x-supportedDeviceType": [
              "Switch PVOS"
            ],
            "type": "string",
            "format": "pattern",
            "x-patternSources": "cak",
            "x-patterns": "'[a-f0-9]{1,64}'",
            "maxLength": 9999,
            "description": "Secure Connectivity Assocation Key (CAK) associated with this\npolicy."
          }
        }
      },
