		/*
		 * insertMyAccountHTML() - insert My Account flyout to the page
		 */
		insertMyAccountHTML: function() {
			if( !this.myAccountHtmlInserted && (document.getElementById(this.myAccountHTMLContainter) != null) ) {
				document.getElementById(this.myAccountHTMLContainter).innerHTML = this.renderMyAccountHTML();
				this.myAccountHtmlInserted = true;
			}
		},


		/*
		 * insertCartIndicator() - loads cart indicator to the page. If Cart is not empty, number of items in cart will be shown over the cart icon.
		 */
		insertCartIndicator: function () {
			if ( document.getElementById(this.cartIndicatorContainter) == null ) {
				this.log('Cannot set Mini Cart Indicator on the page: DIV ' + this.cartIndicatorContainter + 'does not exist.');
				return false;
			}

			if (!this.isCartEmpty()) {
				document.getElementById(this.cartIndicatorContainter).innerHTML = this.cartNumItems;
			}
			else {
				document.getElementById(this.cartIndicatorContainter).innerHTML = '';
			}

			return true;
		},


		/*
		 * initStoreMyAccountFlyout() - this method initalizes Store's My Account flyout (if enabled), and in case user is logged in to store, inserts to header icon the login indicator
		 */
		initStoreMyAccountFlyout: function () {
			if ( document.getElementById("hpe-my-account-container") != null ) {
				this.loadMyAccount();
			}

			if ( document.getElementById(this.userLoggedInIndicatorContainter) == null ) {
				this.log('Cannot load User Logged In indicator to the page: DIV ' + this.userLoggedInIndicatorContainter + 'does not exist.');
				return false;
			}

			var indicatorContainer = document.getElementById(this.userLoggedInIndicatorContainter);
			if (this.isUserLoggedIn()) {
				indicatorContainer.innerHTML = '&middot;';
			}
			else {
				indicatorContainer.innerHTML = '';
			}
		},


		/*
		 * insertUserLoggedInIndicator() - checks if user is logged in to store, and inserts to header icon indicator that user is logged in, if necessary
		 */
		insertUserLoggedInIndicator: function () {
			if ( document.getElementById(this.userLoggedInIndicatorContainter) == null ) {
				this.log('Cannot load User Logged In indicator to the page: DIV ' + this.userLoggedInIndicatorContainter + 'does not exist.');
				return false;
			}

			var indicatorContainer = document.getElementById(this.userLoggedInIndicatorContainter);
			if (this.isUserLoggedIn()) {
				indicatorContainer.innerHTML = '&middot;';
			}
			else {
				indicatorContainer.innerHTML = '';
			}
		},


		/*
		 * loadCart() - loads cart content onto the page
		 */
		loadCart: function () {
			if (!this.cartLoaded) {
				var self = this;

				if ( document.getElementById(this.cartHTMLContainter) == null ) {
					this.log('Cannot load Mini Cart to the page: DIV ' + this.cartHTMLContainter + 'does not exist.');
					return false;
				}

				this.getCartJson(function (json) {
						self.cartJsonData = json;
						if (self.cartJsonData) {
							self.insertCartCSS();
							self.insertCartHTML();
							self.cartLoaded = true;
							self.log('Mini Cart Loaded.');
						}
						else {
							self.log('Mini Cart error - cannot retrieve cart data.');
						}
					});
			}
			return true;
		},


		/*
		 * loadMyAccount() - loads My Account content onto the page
		 */
		loadMyAccount: function () {
			if (!this.myAccountLoaded) {
				this.insertCartCSS();
				this.insertMyAccountHTML();
				this.displayLoggedInSection();
				/* Initialize events necessary to handle Order Status */
				var self = this;
				/* Event to execute order status logic when user clicks "Track My Order" button */
				$("#hpehf-myaccount-track").click(function() {
					var ordNumber = $("#hpehf-myaccount-input-on").val();
					if ( (/^\d+$/.test(ordNumber)) && (ordNumber.length == 10) && ( (/^71/.test(ordNumber)) || (/^00/.test(ordNumber)) ) ) {
						if (/^71/.test(ordNumber)) {
							window.location.href = 'https://b2b.hpe.com/login/auth';
						}
						if (/^00/.test(ordNumber)) {
							window.location.href = this.storeBaseUrl + '/' + self.cc + '/' + self.ll + '/login';
						}
					}
					else {
						$(".hpehf-myaccount-order-status").addClass('hpehf-myaccount-has-error');
					}
					return false;
				});

				/* In case user hits "Enter" key after entering order number, execute "Track My Order" click event to trigger the order status logic. */
				$("#hpehf-myaccount-input-on").keydown(function(event) {
					if (event.keyCode === 13) {
						$("#hpehf-myaccount-track").click();
						return false;
					}
				});

				/* event handler for FOCUS IN */
				$("#hpehf-myaccount-input-on").on('focusin', function() {
					/* if error message is displayed, remove the "error" classes */
					if ($(".hpehf-myaccount-order-status").hasClass('hpehf-myaccount-has-error')) {
						$(".hpehf-myaccount-order-status").removeClass('hpehf-myaccount-has-error');
					}
					/* when the input field is focused, add "focused" classes to change cursor and bottom border color to green */
					$(".hpehf-myaccount-order-status").addClass('hpehf-myaccount-order-status-focused');
				});

				/* event handler for FOCUS OUT */
				$("#hpehf-myaccount-input-on").on('focusout', function() {
					/* when the input loses focus, remove the "focused" classes and revert to original display */
					$(".hpehf-myaccount-order-status").removeClass('hpehf-myaccount-order-status-focused');
				});

				/* event handler for INPUT / when user starts typing */
				$("#hpehf-myaccount-input-on").on('input', function() {
					/* if error message is displayed, remove all the "error" classes and change back to focused state */
					if ($(".hpehf-myaccount-order-status").hasClass('hpehf-myaccount-has-error')) {
						$(".hpehf-myaccount-order-status").removeClass('hpehf-myaccount-has-error');
					}
				});

				this.myAccountLoaded = true;
			}
			return true;
		},


		/**
		 * displayLoggedInSection() - a custom message is being displayed instead of the login buttons section in my account flyout if the user is logged in
		 */
		displayLoggedInSection: function() {
			if (this.isUserLoggedIn()) {
				var loginSectionContainer = document.getElementById('hpehf-myaccount-logins');
				var loggedInSectionContainer = document.getElementById('hpehf-myaccount-logged-in');

				if (!loginSectionContainer.classList.contains('hidden')) {
					loginSectionContainer.classList.add('hidden');
				}
				if (loggedInSectionContainer.classList.contains('hidden')) {
					loggedInSectionContainer.classList.remove('hidden');
				}
			}
		},


		/*
		 * init(config) - initializes HPE_MINI_CART object with the provided configuration object
		 */
		init: function (config) {
			var self = this;

			// check if cart was initalized already - to not initalize it twice.
			if (this.cartInitialized) {
				return true;
			}

			this.log('Mini Cart Init start.');
			this.cartInitialized = true;
			this.cfg = config;
			this.detectLocale();

			// Initialize hfwsStaticPath and storeBaseUrl from Cart container DIV, if available.
			if ( document.getElementById(this.cartHTMLContainter) != null ) {
				this.log('Cart HTML container DIV exist - trying to read hfws-static-path and store-base-url attributes');

				var cartContainer = $('#' + this.cartHTMLContainter);
				if ( typeof(cartContainer.attr('hfws-static-path')) != 'undefined' ) {
					this.hfwsStaticPath = cartContainer.attr('hfws-static-path');
				}

				if ( typeof(cartContainer.attr('store-base-url')) != 'undefined' ) {
					this.storeBaseUrl = cartContainer.attr('store-base-url');
				}

				// TODO: added workaround to check store-path-prefix until all environments stop using /b2c prefix. In order to not have to change AEM code, if no store-path-prefix defined, then prefix is /b2c. When all store environments migrate to not use /b2c, update code to remove store-path-prefix dependency, and use just store-base-url
				if ( typeof(cartContainer.attr('store-path-prefix')) != 'undefined' ) {
					this.storeBaseUrl = this.storeBaseUrl + cartContainer.attr('store-path-prefix');
				}
				else {
					this.storeBaseUrl = this.storeBaseUrl + '';
				}
			}

			// Build full urls for hfwsImgPath and staticCartJsonURL
			this.hfwsImgPath = this.hfwsStaticPath + this.hfwsImgPath;
			this.staticCartJsonURL = this.hfwsStaticPath + this.staticCartJsonURL;

			// init userHasCart based on cookie information from store
			if(this.userHasCart = this.readCookie(this.userHasCartCookie)) { // if cookie exists
				if(this.userHasCart == "true") {
					this.userHasCart = true;
				}
				else {
					this.userHasCart = false;
				}
			}
			else { // if cookie doesn't exist
				this.userHasCart = false;
			}

			// Initialize indicator for number of cart items - works asyncronously, so we need to pass callback function executed when JSON is loaded:
			this.getCartJson(function (json) {
					self.setCartItemsNumberFromJson(json);
					self.insertCartIndicator();
				});

			//this.insertUserLoggedInIndicator();
			if ( (typeof window.HPEHF !== 'undefined' ) && HPEHF.hfInitialized ) {
				this.initStoreMyAccountFlyout();
			}
			else {
				function _minicart_hpehf_ready_event_listener() {
					document.removeEventListener('HPEHF.READY', _minicart_hpehf_ready_event_listener);
					self.initStoreMyAccountFlyout();
				}
				document.addEventListener('HPEHF.READY', _minicart_hpehf_ready_event_listener);
			}

			this.log('Mini Cart Initialized.');
		}
	}


	/* If page is loaded at the time of execution, intialize cart right away. Otherwise, attach event function to load it when document is complete */
	if (document.readyState == 'interactive' || document.readyState == 'complete') {
			/* Initialize Cart */
			window.HPE_MINI_CART.init(window.HPE_MINI_CART_CFG);
	}
	else {
		/* Check if other onreadystatechange events are already defined, and preserve them */
		var saved_onreadystatechange_function = null;
		if(typeof document.onreadystatechange == 'function') {
			saved_onreadystatechange_function = document.onreadystatechange;
		}
		else {
			saved_onreadystatechange_function = function () { ; }
		}
		document.onreadystatechange = function() {
			saved_onreadystatechange_function();
			if (document.readyState == 'interactive' || document.readyState == 'complete') {
				/* Initialize Cart */
				window.HPE_MINI_CART.init(window.HPE_MINI_CART_CFG);
			}
		}
	}

/*********************************************************/
}

/* for migration from legacy mootools hfws instalations, check if jQuery is included to page, and if not, wait for HFWS to initialize and load jQuery */
if (typeof window.jQuery !== 'undefined') {
	_hpe_mini_cart(window.jQuery);
}
else {
	function _hpehf_ready_event_listener() {
		document.removeEventListener('HPEHF.READY', _hpehf_ready_event_listener);
		_hpe_mini_cart(window.jQuery);
	}
	document.addEventListener('HPEHF.READY', _hpehf_ready_event_listener);
}
