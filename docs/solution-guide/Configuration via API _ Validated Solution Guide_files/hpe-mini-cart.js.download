				if (document.getElementById('request_checkout').classList.contains('disable')) {
					document.getElementById('request_checkout').onclick = function() { return false }
				}
				if (document.getElementById('request_quote').classList.contains('disable')) {
					document.getElementById('request_quote').onclick = function() { return false }
				}

